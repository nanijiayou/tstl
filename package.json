{
  "name": "tstl",
  "version": "0.0.1",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "main": "./dist/tstl.es.js",
  "module": "./dist/tstl.umd.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tstl.es.js",
      "require": "./dist/tstl.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --mode web",
    "dev:mp": "vite --mode mp",
    "serve": "vite preview",
    "cz": "cz",
    "release": "standard-version",
    "lint:script": "tslint \"src/**/*.ts\" --fix",
    "prettier": "prettier --write \"src/**/*.ts\" --loglevel warn",
    "fix": "npm run prettier & npm run lint:script",
    "test": "jest",
    "build": "npm run fix & tsc --emitDeclarationOnly & vite build",
    "build:mp": "npm run fix & tsc --emitDeclarationOnly & vite build --mode mp",
    "build:demo": "npm run fix & tsc --emitDeclarationOnly & vite build --mode demo",
    "build:doc": "npm run build & npm run doc",
    "prepare": "husky install",
    "doc:core": "typedoc --out docs/core src/core/doc.ts src/core/types.ts --cleanOutputDir true",
    "doc:adapter": "typedoc --cleanOutputDir true",
    "look:doc": "anywhere -d ./docs",
    "look:doc:core": "anywhere -d ./docs/core",
    "look:doc:adapter": "anywhere -d ./docs/adapter"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@johmun/vue-tags-input": "^2.1.0",
    "@types/jest": "^27.0.2",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.23",
    "babel-jest": "^27.3.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "prettier-tslint": "^0.4.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4",
    "vite": "^2.6.4",
    "vite-jest": "^0.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "vue": "^3.2.23"
  }
}
